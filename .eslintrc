{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"semi": "off",
		"linebreak-style": "off",
		"indent": "off",
		"no-tabs": "off",
		"comma-dangle": "off",
		"arrow-parens": "off",
		"eol-last": "off",
		"object-curly-spacing": "off",
		"max-len": "off"
	},
	"extends": [
		"eslint:recommended",
		"google"
	]
}